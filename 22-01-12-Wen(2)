#include <iostream>

using namespace std;

typedef struct _person {

	int age;
	string name;

} Person;

typedef struct _car {

	int velo;
	string model;

} Car;

void introduce(Person p) {
	cout << p.name << ", " << p.age << endl;
}

void carInfo(Car c) {
	cout << "모델 명 : " << c.model << endl;
	cout << "베기량 : " << c.velo << endl;
}


int main() {

	Person p1;
	p1.age = 20;
	p1.name = "홍길동";
	cout << p1.age << endl;
	cout << p1.name << endl;
	introduce(p1);

	Car car;
	car.model = "아반떼";
	car.velo = 120;
	carInfo(car);

}
